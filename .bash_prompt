vc_branch(){
    local git_result
    local svn_result
    echo $git_result
    git_result=1
    echo git_result
    svn_result=1
    $(git branch &>/dev/null)
    git_result=$?
    echo $git_result
    $(svn status &>/dev/null)
    svn_result=$?
    echo $git_result
    if [ "$git_result" == "0" ] ; then
        echo "is git branch"
    elif [ "$svn_result" == "0" ] ; then
        echo "is svn branch"
    fi
    # FIXME: figure this out some time
}
vc_status(){
    VCBRANCH=""
    # FIXME: VC=$(vc_branch)
    VC=""
    return 0
}

export PROMPT_COMMAND=vc_status
export PS1="\u@\h:\w ${VC}$ "
export PS2="> "
