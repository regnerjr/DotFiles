[user]
	name = John Regner
	email = j.regner@astronautics.com
[credential]
	helper = cache --timeout=14400
[color]
	diff = auto
	status = auto
	branch = auto
	ui = true
[core]
	safecrlf = true
	autocrlf = false
	editor = vim
	whitespace = trailing-space,space-before-tab,indent-with-non-tab
[help]
	autocorrect = 1
[alias]
	co = checkout
	ci = commit
	st = status
	br = branch
	hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
	type = cat-file -t
	dump = cat-file -p
	l = log --oneline --decorate
	fetch = fetch -v
	bs = status -bs
	remotes = remote -v
	branches = branch -a
	tags = tag -l
